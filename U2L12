Q = [[0.5, 1.4, 1], [1, 2, 0.5]]
P = [[2.20], [3.10], [2.60]]


def matmult(mat1, mat2):
  multarr =[]
  for rownum in range(len(mat1)):
    multarr.append([0] * len(mat2[0]))
  
  for rowcount in range(len(mat2[0]) - 1):
    for count in range(len(mat1) - 1):
      multarr[rowcount][columncount] = (mat1[rowcount][0]*mat2[0][columncount]) + (mat1[rowcount][1][columncount])
      
  return multarr
      
    
  
print(matmult(Q, P))

